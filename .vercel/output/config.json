{
	"version": 3,
	"routes": [
	  {
		"src": "^/_astro/(.*)$",
		"headers": {
		  "cache-control": "public, max-age=31536000, immutable"
		},
		"continue": true
	  },
	  {
		"handle": "filesystem"
	  },
	  {
		"src": "^\\/_image$",
		"dest": "_render"
	  },
	  {
		"src": "/api/login",
		"dest": "api/login.ts",
		"methods": ["POST"]
	  },
	  {
		"src": "/api/register",
		"dest": "api/register.ts",
		"methods": ["POST"]
	  }
	]
  }
  