			"dest": "_render"
		},
		{
			"src": "^\\/foro\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/login\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/notfound\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/novedades\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/nps-individual\\/?$",
			"dest": "_render"
